{
  "productName": "easy-proton-ui",
  "version": "0.1.0",
  "identifier": "com.wucaicai.easy-proton",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "EasyProton",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "rpm",
      "deb",
      "appimage"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "WuCaiCaiCai",
    "linux": {
      "shortDescription": "Lightweight Proton/PFX Manager",
      "longDescription": "A simple tool to manage Wine Prefixes and Proton versions for Windows games on Linux."
    },
    "rpm": {
      "depends": [
        "webkit2gtk4.1"
      ]
    }
  }
}